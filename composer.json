{

  "name": "httpbin/httpbin",
  "description": "httpbin port to php: test your http call and api",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/gsouf/httpbin-php",
  "license": "MIT",

  "authors": [
    {
      "name": "Soufiane GHZAL",
      "homepage": "https://github.com/gsouf"
    }
  ],

  "autoload":{
    "psr-4" : {
      "HttpBin\\"   :   "src/"
    }
  },

  "autoload-dev":{
    "psr-4" : {
      "HttpBin\\Test\\"   :   "test/suites"
    }
  },

  "require": {
    "php": ">=5.5.0",
    "aura/router": "~3.0.0",
    "zendframework/zend-diactoros": "~1.1",
    "minime/annotations": "~2.3",
    "symfony/process": "~2",
    "gsouf/simple-curl": "~0.1",
    "justinrainbow/json-schema": ">=1.4"
  },

  "require-dev":{
    "phpunit/phpunit": "4.1.2",
    "codeclimate/php-test-reporter": "dev-master",
    "squizlabs/php_codesniffer": "~2",
    "guzzlehttp/guzzle": "~6.1"
  }
}
